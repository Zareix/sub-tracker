		"@trpc/server": "^11.9.0",
		"@types/nodemailer": "^7.0.9",
		"better-auth": "^1.4.18",
		"bun-fetch-mock": "^1.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
