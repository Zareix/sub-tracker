{
	"name": "sub-tracker",
	"version": "1.21.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "bun run --bun next build",
		"build:analyze": "ANALYZE=true bun run --bun next build",
		"db:generate": "bunx drizzle-kit generate",
		"db:push": "drizzle-kit push",
		"db:studio": "drizzle-kit studio",
		"dev": "bun run --bun next dev --turbo",
		"start": "next start",
		"lint": "biome check .",
		"check": "biome check --write .",
		"email:dev": "email dev -d src/server/email/templates -p 3001",
		"auth:generate": "bunx --bun @better-auth/cli generate --config src/server/auth.ts",
		"test": "bun run --parallel test:int test:e2e",
		"test:int": "bun test --preload ./src/tests/integrations/_setup.ts ./src/tests/integrations",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:report": "playwright show-report"
	},
	"packageManager": "bun@1.3.8",
	"dependencies": {
